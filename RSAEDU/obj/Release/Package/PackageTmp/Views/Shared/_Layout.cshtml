                                <div class="dropdown-menu" aria-labelledby="download">
                                    <a class="dropdown-item" href="@Url.Action("GradeIndex", "ExamInfo")">Grade Parameter</a>
                                    <a class="dropdown-item" href="@Url.Action("Index", "ExamInfo")">Exam Name</a>
                                    <a class="dropdown-item" href="@Url.Action("Index", "ExamSettings")">Exam Marks Parameter</a>
                                </div>
                            </li>

