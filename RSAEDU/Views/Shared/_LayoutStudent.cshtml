<body>
    <div class="fixed-top">
        <div class="navbar row text-center" style="color:white ;background-color:#808080">
            <div class="col-md-2 col-sm-2">

            </div>
            <div class="col-md-8">
                <h3 class="text-center" style="color:white;font-family:Verdana !important;">KUMUDINI GOVT. COLLEGE, TANGAIL</h3>
            </div>
            <div class="col-md-2 col-sm-2">

            </div>

        </div>
        <div class="navbar navbar-expand-lg navbar-dark bg-primary">

            <div class="container">
                <a href="/Home/Index" class="navbar-brand">

                    <img src="~/Resources/logo.jpg" height="80" width="80" />
                </a>

                @if(User.Identity.IsAuthenticated)
                {
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarResponsive">
                        <ul class="navbar-nav">                    
                        
                            <li class="nav-item">
                                <a class="nav-link" href="@Url.Action("Index","Report")" id="download">Report</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/Account/Manage" id="download">Change Password</a>
                            </li>

                        </ul>
                        <ul class="nav navbar-nav ml-auto">
                            @Html.Partial("_LoginPartial")
                        </ul>
                    </div>
                }
            </div>
        </div>

    </div>
  
    <div class="container" style="padding-top:10%">

        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - RSA Info Tech all right reserved.</p>
        </footer>

    </div>

    @Scripts.Render("~/bundles/jquery")  


    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

